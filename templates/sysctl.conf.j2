# {{ ansible_managed }}
# sysctl_conf: {{ sysctl_conf|length }}
{% if sysctl_conf|length > 0 %}

{%   for key, value in sysctl_conf.items()|sort %}
{%     if value is string %}
{{ key }} = {{ value|string }}
{%     elif value is number %}
{{ key }} = {{ value|int }}
{%     else %}
{{ key }} = {{ value }}
{%     endif %}
{%   endfor %}
{% endif %}
